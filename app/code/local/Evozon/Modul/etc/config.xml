<?xml version="1.0"?>
<config>
    <modules>
        <evozon_modul>
            <version>1.0.4</version>
        </evozon_modul>
    </modules>
    <global>
        <models>
            <evozon_modul>
                <class>Evozon_Modul_Model</class>
                <resourceModel>evozon_modul_resource</resourceModel>
            </evozon_modul>
            <evozon_modul_resource>
                <class>Evozon_Modul_Model_Resource</class>
                <entities>
                    <modelu>
                        <table>tabela_modelumeu</table>
                    </modelu>
                </entities>
            </evozon_modul_resource>
        </models>
        <resources>
            <evozon_modul_setup>
                <setup>
                    <module>evozon_modul</module>
                    <class>Evozon_Modul_Model_Resource_Setup</class>
                </setup>
            </evozon_modul_setup>
            <evozon_modul_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </evozon_modul_write>
            <evozon_modul_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </evozon_modul_read>
        </resources>
        <blocks>
            <evozon_modul>
                <class>Evozon_Modul_Block</class>
            </evozon_modul>
            <third>
                <class>Evozon_Modul_Block</class>
            </third>
            <evozon_modul_db>
                <class>Evozon_Modul_Block</class>
            </evozon_modul_db>
            <evozon_modul_banners>
                <class>Evozon_Modul_Block</class>
            </evozon_modul_banners>
        </blocks>
    </global>
    <frontend>
        <routers>
            <modul>
                <use>standard</use>
                <args>
                    <module>evozon_modul</module>
                    <frontName>modul</frontName>
                </args>
            </modul>
        </routers>
        <layout>
            <updates>
                <evozon_modul>
                    <file>theme/evozon_modul.xml</file>
                </evozon_modul>
                <third>
                    <file>theme/third.xml</file>
                </third>
                <evozon_modul_db>
                    <file>theme/db.xml</file>
                </evozon_modul_db>
                <evozon_modul_banners>
                    <file>theme/banner.xml</file>
                </evozon_modul_banners>
            </updates>
        </layout>
    </frontend>
</config>
